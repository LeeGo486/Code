<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	
    <package name="jSupplierNh" extends="struts-default">
    <!-- 注释： -->
    	<action name="execute" class="jSupplierNhAction" method="execute">
    		<result name="success">/index.jsp</result>
    	</action>
    	
    	<action name="supplierSave" class="jSupplierNhAction" method="supplierSave">
    		<result name="success">/listSupplier.jsp</result>
    	</action>
    	
    	<action name="listSupplier" class="jSupplierNhAction" method="listSupplier">
    		<result name="success">/listSupplier.jsp</result>
    	</action>
    	
    	<action name="deleteSupplier" class="jSupplierNhAction" method="deleteSupplier">
    		<result name="success">/listSupplier.jsp</result>
    	</action>
    </package>
   

</struts>
