<table class="AvTable">
    <thead>
        <tr>
            <th style="width: 8%">项目</th>
            <th style="width: 25%">开始时间</th>
            <th style="width: 25%">结束时间</th>
            <th style="width: 10%">状态</th>
        </tr>
    </thead>
    <tbody>
        @foreach (System.Data.DataRow dr in ViewBag.OutGet.Rows)
        {
            <tr>
                <th>@dr["c_name"]</th>
                <th>@dr["c_begindate"]</th>
                <th>@dr["c_enddate"]</th>
                <td>@dr["c_applystatus"]</td>
            </tr>
        }
    </tbody>
</table>
