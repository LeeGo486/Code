<script type="text/javascript">
    $(function () {
        $('#income').highcharts({
            chart: {
                plotBackgroundColor: null,
                plotBorderWidth: null,
                plotShadow: false,
                height: 250,
                backgroundColor: null
            },
            title: {
                text: '收入走势图',
                style: {
                    "fontSize": "14px"
                }
            },
            subtitle: {
                text: '点击节点显示明细'
            },
            xAxis: [{
                categories: ['1月', '2月', '3月', '4月', '5月', '6月', '7月', '8月', '9月', '10月', '11月', '12月']
            }],
            yAxis: {
                title: {
                    text: null
                }
            },
            plotOptions: {
                series: {
                    cursor: 'pointer',
                    events: {
                        click: function (event) {
