                data: [3000, 3500, 3200, 4000, 3500, 3800, 4000, 4500, 3200, 3600, 4500, 3200],
                tooltip: {
                    valueSuffix: null
                }
            }],
            credits: {
                enabled: false
            }
        });
    });
</script>
