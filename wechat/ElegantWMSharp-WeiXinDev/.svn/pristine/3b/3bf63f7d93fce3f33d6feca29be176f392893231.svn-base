<table class="AvTable">
    <thead>
        <tr>
            <th style="width: 25%">加班类型</th>
            <th style="width: 20%">开始时间</th>
            <th style="width: 20%">结束时间</th>
            <th>审批状态</th>
        </tr>
    </thead>
    <tbody>
        @foreach (System.Data.DataRow dr in ViewBag.OverTimeGet.Rows)
        {
            <tr>
                <th>@dr["c_attitemname"]</th>
                <th>@dr["c_begindate"]</th>
                <th>@dr["c_enddate"]</th>
                <td>@dr["c_applystatus"]</td>
            </tr>
        }
    </tbody>
</table>
