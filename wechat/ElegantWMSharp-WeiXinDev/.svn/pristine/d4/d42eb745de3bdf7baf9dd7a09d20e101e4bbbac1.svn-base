namespace ElegantWM.EntityModel
{
    public class Shop_EventFeedback : Entity
    {
        public Shop_EventFeedback() { }

		/// <summary>
		/// 活动主题
		/// </summary>
		public Guid Theme { get; set; }

        /// <summary>
        /// 活动主题名称
        /// </summary>
        public string Name { get; set; }

		/// <summary>
		/// 店铺ID
		/// </summary>
		public string DepotId { get; set; }

        /// <summary>
        /// 店铺名称
        /// </summary>
        public string DepotName { get; set; }

        /// <summary>
        /// 店铺大区
        /// </summary>
        public string Branch { get; set; }

		/// <summary>
		/// 活动目标
		/// </summary>
		public string Global { get; set; }
		
		/// <summary>
		/// 计划人数
		/// </summary>
		public int PlanNum { get; set; }
		
		/// <summary>
		/// 答应人数
		/// </summary>
		public int RespondNum { get; set; }
		
		/// <summary>
		/// 实际人数
		/// </summary>
		public int FactNum { get; set; }
		
		/// <summary>
		/// 购买人数
		/// </summary>
		public int BuyNum { get; set; }
		
		/// <summary>
		/// 购买金额
		/// </summary>
		public decimal Price { get; set; }
		
		/// <summary>
		/// 活动日期
		/// </summary>
		public DateTime EventDay { get; set; }

        /// <summary>
        /// 创建人ID
        /// </summary>
        public string CreateUserId { get; set; }
		

         
    }
}
