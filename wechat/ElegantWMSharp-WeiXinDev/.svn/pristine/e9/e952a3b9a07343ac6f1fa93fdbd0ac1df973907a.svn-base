namespace ElegantWM.EntityModel
{
    public class Game_Record : Entity
    {
        public Game_Record() { }

		/// <summary>
		/// 微信ID
		/// </summary>
		public string Oid { get; set; }
		
		/// <summary>
		/// 昵称
		/// </summary>
		public string Nickname { get; set; }
		
		/// <summary>
		/// 组别
		/// </summary>
		public int Group { get; set; }
		
		/// <summary>
		/// 奖品ID
		/// </summary>
		public Guid AwardId { get; set; }
		
		/// <summary>
		/// 奖品名称
		/// </summary>
		public string AwardName { get; set; }

        /// <summary>
        /// 奖品序列
        /// </summary>
        public string Seq { get; set; }

		/// <summary>
		/// 中奖数量
		/// </summary>
		public int Num { get; set; }
		
		/// <summary>
		/// 是否中奖
		/// </summary>
		public int isAward { get; set; }
		
		/// <summary>
		/// 参与次数
		/// </summary>
		public int doTimes { get; set; }
		
		/// <summary>
		/// 链接次数
		/// </summary>
		public int linkedTimes { get; set; }
		

         
    }
}
