<%@ Master Language="C#" MasterPageFile="~/MasterPage/Base.Master" AutoEventWireup="true"
    CodeBehind="Modal.master.cs" Inherits="F23Web.Public.MasterPage.Modal" %>
<asp:Content ID="Content1" ContentPlaceHolderID="head" runat="server">
    <base target="_self" />
    <style type="text/css">
        body, html
        {
            overflow: hidden;
        }
    </style>
    <asp:ContentPlaceHolder ID="head" runat="server">
    </asp:ContentPlaceHolder>
</asp:Content>
<asp:Content ID="Content2" ContentPlaceHolderID="ContentPlaceHolder1" runat="server">
    <div class="x-page">
        <asp:ContentPlaceHolder ID="ContentPlaceHolder1" runat="server">
        </asp:ContentPlaceHolder>
        <div class="buttonHolder">
            <Base:ButtonItem ID="btnsubmit" runat="server" Text="确 定" CssClass="primaryAction"
                Callback="e_submit" Enabled="false" Type="button" Width="120px" />
            <Base:ButtonItem ID="btncancel" runat="server" Text=" 取 消" CssClass="primaryAction"
                Callback="e_cancel" Type="button" Width="120px" HotKey="esc" />
        </div>
    </div>
</asp:Content>
