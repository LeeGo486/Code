@model EPManageWeb.Entities.Models.User
@{
    Layout = "~/Views/Shared/_AjaxLayout.cshtml";
}
<div class="control-group">
    <label class="control-label" for="UserName">用户名</label>
    <div class="controls">
        @Html.TextBoxFor(t => t.UserName)
    </div>
</div>
<div class="control-group">
    <label class="control-label" for="Password">密码</label>
    <div class="controls">
        @Html.TextBoxFor(t => t.Password)
    </div>
</div>
<div class="control-group">
    <label class="control-label" for="RealName">真实姓名</label>
    <div class="controls">
        @Html.TextBoxFor(t => t.RealName)
    </div>
</div>
<div class="control-group">
    <div class="controls">
        <button type="button" class="btn" id="btnReg">注册</button>
        <button class="btn" id="btnClose" data-dismiss="modal" aria-hidden="true">关闭</button>
    </div>
</div>
<script type="text/javascript" src="@Url.Content("~/Scripts/Reg.js")"></script>
