<!DOCTYPE html>
<html>
<head>
    <title>制版流程操作</title>
    <script src="../../jquery-easyui-1.3.4/js_hzycfg.js" type="text/javascript"></script>
    <script src="../../jquery-easyui-1.3.4/jquery.min.js" type="text/javascript"></script>
    <script src="../../jquery-easyui-1.3.4/jquery.cookie.js" type="text/javascript"></script>
    <link id="easyuiTheme" href="../../jquery-easyui-1.3.4/themes/default/easyui.css" rel="stylesheet" type="text/css" />
    <script src="../../jquery-easyui-1.3.4/changeEasyuiTheme.js" type="text/javascript"></script>
    <link href="../../jquery-easyui-1.3.4/themes/icon.css" rel="stylesheet" type="text/css" />
    
</head>
<body class="easyui-layout" data-options="fit:true">
    <div region="north" style="height: 50px; overflow: hidden" data-options="split:false,border:false">
        <div style="padding-top: 10px; width: 100%; overflow: hidden">
            <form id="frmSeach">
                <table border="0">
                    <tr>
                        <td>品牌：<input id="cmbBrand" name="cmbBrand" style="width: 100px">
                            查询方式：<input id="cmbSelectType" name="cmbSelectType" style="width: 100px">
                        </td>
                        <td>
                            <div id="date" style="display: inline-block; *zoom: 1; *display: inline;">
                                开始时间：<input class="easyui-datebox" id="txtBeginDate" name="txtBeginDate" data-options="formatter:Formater.Date" style="width: 100px" />
                                结束时间：<input class="easyui-datebox" id="txtEndDate" name="txtEndDate" data-options="formatter:Formater.Date" style="width: 100px" />
                            </div>

                            <div id="orderPlacing" style="display: none;">
                                季节：<input id="cmbdhhjj" name="cmbdhhjj" style="width: 200px" data-options="valueField:'id',textField:'text'">
                                版师：<input id="txtpatterner" name="txtpatterner" style="width: 80px" />
                                品类：<input id="txtpl" name="txtpl" style="width: 80px" data-options="valueField:'id',textField:'text'">
                            </div>
                            <div id="productid" style="display: none;">大货号: <input type="text" name="txtproductid" id="txtproductid" /></div>
                            <div id="design" style="display: none;">编号：<input id="txtDesignerId" name="txtDesignerId" class="easyui-validatebox" data-options="required:true" /></div>

                        </td>
                        <td>
                            <a href="#" id="btn_Search" iconcls="icon-search" onclick="SearchClick()" class="easyui-linkbutton"
                                data-options="plain:true">搜索</a>
                        </td>
                    </tr>
                </table>
            </form>
        </div>
    </div>
    <div region="center" split="true" id="mainPanle">
        <table id="tabList">
        </table>
    </div>
    <!-- 样衣打印窗口 -->
    <div id="winSample">
        <!-- 标签 -->
        <div id="tt" class="easyui-tabs">
            <!-- 查看Bom -->
            <div title="选择样衣">
                <table id="win_list"></table>
            </div>
            <!-- 确认样工艺 -->
            <div title="确认样工艺包录入功能与查询">
                <div style="width: 100%;height: 60px">
                    <div style="padding-top: 10px; width: 100%;height:100%">
                        订货会季节：<input id="txtcmbdhhjj" name="txtcmbdhhjj" readonly="true" style="width: 200px">
                        样衣号：<input id="txtdyyyno" name="txtdyyyno" readonly="true" style="width: 200px">
                        大货号：<input id="dahuohao" readonly="true" style="width: 200px" >
                        <a rel="buttom" href="#" id="smpShtBtn"></a>
                    </div>
                </div>
                <!-- center -->
                <div class="easyui-layout" style="width:100%;height:590px">
                    <!-- west -->
                    <div region="west" data-options="split:true,border:true" style="width: 420px;">
                        <table id="tabPrint"></table>
                    </div>
                    <!-- center -->
                    <div region="center">
                        <form id="formSub" name="formSub" method="post">
                            <table style="font-size: 9pt;" cellpadding="2px;" id="tbSub">
                                <tr>
                                    <td id="SampleClass">款别：<br />
                                        <input id="EPVA" type="checkbox" checked="checked" />A
                                        <input id="EPVB" type="checkbox" checked="checked" />B
                                        <input id="EPVC" type="checkbox" checked="checked" />C
                                        <input id="EPVD" type="checkbox" checked="checked" />D
                                    </td>
                                </tr>
                                <tr>
                                    <td>面辅料代用信息：<br />
                                        <textarea id="MaterialsUsageInformation" name="txtMaterialsUsageInformation" cols="20" rows="2" style="width: 300px; height: 40px;"></textarea>
                                    </td>
                                </tr>
                                <tr>
                                    <td id="SampleRequirement6">面料性能信息：<br />
                                        <input id="EPVRequirement6Sub1" type="checkbox" checked="checked" />起毛、起球
                                        <input id="EPVRequirement6Sub2" type="checkbox" checked="checked" />拔丝、排丝<br />
                                        <input id="EPVRequirement6Sub3" type="checkbox" checked="checked" />搭色
                                        <input id="EPVRequirement6Sub4" type="checkbox" checked="checked" />褪色
                                        <input id="EPVRequirement6Sub5" type="checkbox" checked="checked" />缩率大
                                    </td>
                                </tr>
                                <tr>
                                    <td id="SampleRequirement1">绣花,印花要求：<br />
                                        <input id="EPVRequirement1Sub1" type="checkbox" checked="checked" />裁片
                                        <input id="EPVRequirement1Sub2" type="checkbox" checked="checked" />面料
                                        <input id="EPVRequirement1Sub3" type="checkbox" checked="checked" />成品
                                    </td>
                                </tr>
                                <tr>
                                    <td id="SampleRequirement3">排版要求：<br />
                                        <input id="EPVRequirement3Sub1" type="checkbox" checked="checked" />倒毛
                                        <input id="EPVRequirement3Sub2" type="checkbox" checked="checked" />顺毛
                                        <input id="EPVRequirement3Sub3" type="checkbox" checked="checked" />定位<br />
                                        <input id="EPVRequirement3Sub7" type="checkbox" checked="checked" />直裁
                                        <input id="EPVRequirement3Sub8" type="checkbox" checked="checked" />横裁
                                        <input id="EPVRequirement3Sub9" type="checkbox" checked="checked" />斜裁<br />
                                        <input id="EPVRequirement3Sub4" type="checkbox" checked="checked" />对花
                                        <input id="EPVRequirement3Sub5" type="checkbox" checked="checked" />对格
                                        <input id="EPVRequirement3Sub6" type="checkbox" checked="checked" />对条
                                    </td>
                                </tr>
                                <tr>
                                    <td id="SampleRequirement2">推码要求：<br />
                                        <input id="EPVRequirement2Sub1" type="checkbox" checked="checked" />按标准
                                        <input id="EPVRequirement2Sub2" type="checkbox" checked="checked" />其他
                                    </td>
                                </tr>
                                <tr>
                                    <td id="SampleRequirement7">洗涤要求：<br />
                                        <input id="EPVWaterScrubbing" type="checkbox" checked="checked" />水洗
                                        <input id="EPVDryCleaning" type="checkbox" checked="checked" />干洗
                                        <input id="EPVHandWash" type="checkbox" checked="checked" />手洗
                                        <input id="EPVMachineWash" type="checkbox" checked="checked" />机洗
                                    </td>
                                </tr>
                                <tr>
                                    <td>其他：<br />
                                        <textarea id="SampleRequirement4" name="txtSampleRequirement4" cols="20" rows="2" style="width: 300px; height: 40px;"></textarea>
                                    </td>
                                </tr>
                                <tr>
                                    <td>技术要求：<br />
                                        <textarea id="SampleRequirement5" name="txtSampleRequirement5" cols="20" rows="2" style="width: 300px; height: 40px;"></textarea>
                                    </td>
                                </tr>
                                <tr>
                                    <td>样衣师：<br />
                                        <input id="EPAsampleMachinest" name="txtEPAsampleMachinest" style="width: 100px" data-options="valueField:'id',textField:'text'">
                                    </td>
                                </tr>
                            </table>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- 采购来源窗口 -->
    <div id="winProduct">
        <table id="ProductList"></table>
    </div>
    <script src="../../jquery-easyui-1.3.4/jquery.easyui.min.js" type="text/javascript"></script>
    <script src="../../jquery-easyui-1.3.4/jquery.easyui.datagrid_groupview.js" type="text/javascript"></script>
    <script src="../../jquery-easyui-1.3.4/JS_PrintArea.js" type="text/javascript"></script>
    <script src="../../jquery-easyui-1.3.4/js.extend.datetime.js"></script>
    <script src="../../JS/Lodop/LodopFuncs.js"></script>
    <script src="../../jquery-easyui-1.3.4/js_hzyCommon.js" type="text/javascript"></script>
    <script src="../../jquery-easyui-1.3.4/jquery.numberformatter.js" type="text/javascript"></script>
    <script src="../../JS/PLMAS/PlateMakingProcess.js?Date=20140422" type="text/javascript"></script>
    <script src="../../JS/PLMAS/ConfirmationSamplePackageInput.js?Date=2014042222"></script>
</body>
</html>
