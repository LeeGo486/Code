    <style>
        .btn_select
        {
            text-align: center;
            border: 2px solid #dcdcdc;
            background-color: #E0E0E0;
            /*line-height: 60px;*/
            border-radius: 5px;
            color: #252525;
            width: 80%;
            border-radius: 5px;
            /*font-size: 18px;*/
            text-shadow: none;
            padding: 5px 5px 5px 5px;
            overflow: hidden;
            box-shadow: inset 0px 0px 1px #eee;
            text-shadow: 0px 1px 1px rgba(0,0,0,0.16);
            -moz-border-radius: 1px;
            -webkit-border-radius: 1px;
            -moz-box-shadow: 0px 0px 1px #EEE inset;
            -webkit-box-shadow: 0px 0px 1px #EEE inset;
        }

        .btn_unSelect
        {
            text-align: center;
            border: 2px solid #dcdcdc;
            /*line-height: 30px;*/
            border-radius: 5px;
            color: #656565;
            background: #FFF;
            width: 80%;
            border-radius: 5px;
            /*font-size: 18px;*/
            text-shadow: none;
            padding: 5px 5px 5px 5px;
            overflow: hidden;
            box-shadow: inset 0px 0px 1px #eee;
            text-shadow: 0px 1px 1px rgba(0,0,0,0.16);
            -moz-border-radius: 1px;
            -webkit-border-radius: 1px;
            -moz-box-shadow: 0px 0px 1px #EEE inset;
            -webkit-box-shadow: 0px 0px 1px #EEE inset;
        }

            .btn_unSelect:hover
            {
                background: #E0E0E0;
            }
