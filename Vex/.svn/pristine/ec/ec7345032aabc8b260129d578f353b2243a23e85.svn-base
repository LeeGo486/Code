<!DOCTYPE html>
<html>
<head>
    <title>雅斓ERP-收货管理</title>
    <script src="../../jquery-easyui-1.3.4/js_hzycfg.js" type="text/javascript"></script>
    <script src="../../jquery-easyui-1.3.4/jquery.min.js" type="text/javascript"></script>
    <script src="../../jquery-easyui-1.3.4/jquery.cookie.js" type="text/javascript"></script>
    <link id="easyuiTheme" href="../../jquery-easyui-1.3.4/themes/default/easyui.css"
        rel="stylesheet" type="text/css" />
    <script src="../../jquery-easyui-1.3.4/changeEasyuiTheme.js" type="text/javascript"></script>
    <link href="../../jquery-easyui-1.3.4/themes/icon.css" rel="stylesheet" type="text/css" />
    <script src="../../jquery-easyui-1.3.4/jquery.easyui.min.js" type="text/javascript"></script>
    <script src="../../jquery-easyui-1.3.4/js_hzyCommon.js" type="text/javascript"></script>
    <script src="../../jquery-easyui-1.3.4/jquery.ajaxupload.js" type="text/javascript"></script>
    <script src="../../jquery-easyui-1.3.4/jquery.easyui.datagrid-detailview.js" type="text/javascript"></script>
    <script src="../../jquery-easyui-1.3.4/jquery.numberformatter.js" type="text/javascript"></script>
    <script src="../../JS/Hawk/POReceipt.JS?Date=20140201" type="text/javascript"></script>
</head>
<body class="easyui-layout" data-options="fit:true">
    <div data-options="region:'north',split:false,border:false,height:65">
        <form id="getPOReceipt" method="post" action = "#">
            <table>
                <tr>
                    <td>供应商:</td>
                    <td><input id="cmbSupplierCode" name="cmbSupplierCode" style="width:100px;" /></td>
                    <td>采购单号:</td>
                    <td><input id="cmbPOCode" name="cmbPOCode" style="width:100px;" /></td>
                    <td>到货计划编号:</td>
                    <td><input id="cmbAPCode" name="cmbAPCode" style="width:100px;" /></td>
                    <td>采购员:</td>
                    <td><input id="txtRgUser" name="txtRgUser" style="width:100px;" /></td>
                    <td>供应商采购单编号:</td>
                    <td><input id="txtSPPOCode" name="txtSPPOCode" style="width:100px;" /></td>
                    <td><a href="#" id="btnSearch" onclick="Search()">查询</a></td>
                </tr>
                <tr>
                    <td colspan=4>
                        <a href="#" id="btnCallWindow" onclick="POReceiptWindows('Eligible')">合格品收货</a>
                        <a href="#" id="btnCallWindowOther" onclick="POReceiptWindows('Failure')">不合格品收货</a>
                        <a href="#" id="btnAudit" onclick="btnAudit('')">审核</a>
                    </td>
                </tr>
            </table>
        </form>
    </div>
    <div data-options="region:'center'">
        <table id="tab_list"></table>
    </div>
    <div id="winPOReceiptEdit"> 
        <form id="formPOReceiptEdit" method="post" action = "#">
            <table>
                <tr>
                    <td>收货单号</td>
                    <td><input id="txtReceiptCode" disabled="disabled" style="width:100px;"/></td>
                    <td>采购单号</td>
                    <td><input id="txtPOcode" disabled="disabled" style="width:100px;"/></td>
                    <td>到货计划编号</td>
                    <td><input id="txtAPcode" disabled="disabled" style="width:120px;"/></td>
                    <td>供应商</td>
                    <td><input id="txtSupplier" disabled="disabled" style="width:100px;"/></td>
                    <td>采购员</td>
                    <td><input id="txtPOrguser" disabled="disabled" style="width:100px;"/></td>
                    <td>收货员</td>
                    <td><input id="txtuser" disabled="disabled" style="width:100px;"/></td>
                </tr>
                <tr>
                    <td>不合格原因</td>
                    <td><input id="cmbFailure" name="cmbFailure" style="width:100px;"/></td>
                    <td>收货仓库:</td>
                    <td><input id="cmbWareHouse" name="cmbRP1_WHSP_WarehousePOS"/></td>
                    <td>条码:</td>
                    <td colspan=4><input id="txtBarcode" name="txtBarcode" style="width:200px;" /></td>
                    <td colspan=3>
                        <a href="#" id="btnSubmitSaveAudit" onclick="saveAndAudit()">保存并审核</a>
                        <a href="#" id="btnSubmitSave">保存</a>
                        <a href="#" id="btnCancel">关闭</a>
                    </td>
                </tr>
            </table>
        </form>
            <table id="wintab_list"></table>
    </div>
</body>
</html>
