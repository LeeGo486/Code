<body class="easyui-layout" style="width: 100%; height: 100%;">
    <div style="height: 80px; margin-right: auto; margin-left: auto;" data-options="region:'north',split:false,border:false">
        <div style="padding-top: 10px; width: 100%;">
            <form id="frmSeach">
                <table>
                    <tr>
                        <td>产品线:</td>
                        <td>
                            <input id="txtProductLine" name="txtProductLine" style="width: 100px" />
                        </td>
                        <td>季节:</td>
                        <td>
                            <input id="txtSeason" name="txtSeason" style="width: 200px" />
                        </td>
                        <td>上货日期:</td>
                        <td>
                            <input id="txtExhibitGoodsDate" name="txtExhibitGoodsDate" style="width: 200px" />
                        </td>
                        <td>查询方式:</td>
                        <td>
                            <select id="cmbSelectType" style="width: 80px"></select>
                        </td>
                        <td class="Designer">
                            <input id="txtDesigner" name="txtDesigner" style="width: 100px" />
                        </td>
                        <td class="Pattern">
                            <input id="txtPattern" name="txtPattern" style="width: 100px" />
                        </td>


                    </tr>
                    <tr>
                        <td>样衣数:</td>
                        <td>
                            <input id="txtSampleNum" name="txtSampleNum" style="width: 100px" />
                        </td>
                        <td>品类:</td>
                        <td>
                            <input id="txtCategory" name="txtCategroy" style="width: 200px" />
                        </td>
                        <td>系列:</td>
                        <td>
                            <input id="txtSeries" name="txtSeries" style="width: 200px" />
                        </td>
                        <td>款号:</td>
                        <td>
                            <input id="txtProductName" name="txtProductName" style="width:120px" />
                        </td>
                        <td>
                            <a href="#" id="btn_Search" onclick="btnLoad()" class="easyui-linkbutton"
                                data-options="iconCls:'icon-search',plain:true">加载</a>
                        </td>
                    </tr>
                </table>

            </form>
        </div>
    </div>

    <div data-options="region:'center'" split="true" id="mainPanle">
        <table id="tabList"></table>
    </div>

    <div class="easyui-window" id="winDetail" data-options="closed:true" style="width: 550px; height: 600px" title="明细表">
        <table id="tabDetail"></table>
    </div>
