<!DOCTYPE html>
<html>
<head>
    <title>HZYTOOLS</title>
    <script src="../../jquery-easyui-1.3.4/js_hzycfg.js" type="text/javascript"></script>
    <script src="../../jquery-easyui-1.3.4/jquery.min.js" type="text/javascript"></script>
    <script src="../../jquery-easyui-1.3.4/jquery.cookie.js" type="text/javascript"></script>
    <link id="easyuiTheme" href="../../jquery-easyui-1.3.4/themes/default/easyui.css" rel="stylesheet" type="text/css" />
    <script src="../../jquery-easyui-1.3.4/changeEasyuiTheme.js" type="text/javascript"></script>
    <link href="../../jquery-easyui-1.3.4/themes/icon.css" rel="stylesheet" type="text/css" />
    <script src="../../jquery-easyui-1.3.4/jquery.easyui.min.js" type="text/javascript"></script>
    <script src="../../jquery-easyui-1.3.4/js_hzyCommon.js" type="text/javascript"></script>
    <script src="../../jquery-easyui-1.3.4/jquery.ajaxupload.js" type="text/javascript"></script>
    <script src="../../jquery-easyui-1.3.4/js.extend.datetime.js" type="text/javascript"></script>
    <script src="../../jquery-easyui-1.3.4/jquery.numberformatter.js" type="text/javascript"></script>
    <script src="../../JS/SDK/Tools.js?Date=20141223" type="text/javascript"></script>
</head>
<body class="easyui-layout" data-options="fit:true">
        <div data-options="region:'center',border:false">
            <table id="tab_list" style="width: 100%; height: 98%">
            </table>
    </div>

    <div region="east" split="true" title="明细" style="width:600px;">
        <div class="easyui-layout" split="true" data-options="fit:true">
                <table id="tab_list_Detail">
                </table>
        </div>
    </div>

    <div  title="新增子表" id="divtoolsdetail" style="width: 529px; height: 266px; margin-right: 0px;">
        <form id="formtoolsdetail" title="新增子表" name="formtoolsdetail" method="post">
            <table style="font-size: 9pt;" cellpadding="5px;">
                <tr>
                    <td>PCF_ID：
                    </td>
                    <td>
                        <input id="txtpcf_id" name="txtpcf_id" style="width: 300px" disabled="disabled">
                        <input id="txtpcf_pdc_id" name="txtpcf_pdc_id" style="width: 2px" type="hidden">
                    </td>
                </tr>
                <tr>
                    <td>产品端文件名称：
                    </td>
                    <td>
                        <input id="txtpcf_name" name="txtpcf_name" style="width: 300px" class="easyui-validatebox"
                            data-options="required:true" missingmessage="请输入产品端文件名称,长度不能超过50" maxlength="50">
                </tr>
                <tr>
                    <td>产品端文件版本：
                    </td>
                    <td>
                        <input id="txtpcf_version" name="txtpcf_version" style="width: 300px" class="easyui-validatebox"
                            data-options="required:true" missingmessage="请输入产品端文件版本,长度不能超过3" maxlength="3">
                </tr>
                <tr>
                    <td>产品端文件描述：
                    </td>
                  <td>
                        <textarea id="txtpcf_desp" name="txtpcf_desp" cols="20" rows="2" style="width: 300px; height: 86px;" class="easyui-validatebox"
                            data-options="required:true" missingmessage="请输入产品端文件描述,长度不能超过200" maxlength="200"></textarea>
                    </td>
                </tr>
                <tr>
                    <td>产品端文件大小：
                    </td>
                    <td>
                        <input id="txtpcf_filesize" name="txtpcf_filesize" style="width: 300px" disabled="disabled">
                </tr>
                <tr>
                    <td>文件上传：
                    </td>
                    <td >
                        <input type="text" id="txtpcf_downloadurl" style="width: 250px" disabled="disabled" />
                        <input type="button" id="btnUp" value="浏览"></input>
                    </td>
                </tr>
                <tr>
                    <td colspan="2" align ="center">
                        <a href="#" id="A1" iconcls="icon-ok" data-options="plain:true" onclick="FormDetailSubmit()" class="easyui-linkbutton">保存</a>
                        <a href="#" id="A4" iconcls="icon-undo" onclick="Cancel('A2')" data-options="plain:true"
                            class="easyui-linkbutton">取消</a>
                    </td>
                </tr>
            </table>
        </form>
    </div>


</body>
</html>
