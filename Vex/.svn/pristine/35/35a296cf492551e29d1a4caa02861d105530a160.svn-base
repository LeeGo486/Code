    <script>
        function changePosition(pos) {
            $('#pp').tooltip({
                content: 'aaaaaaa',
                position: pos
            });

            $('#aaaaaaa').tooltip({
                //position: top,
                content: function () {
                    return "pagepagepagepagepage";
                }
            });

            $('#aaaaaaa').tooltip({
                position: "top"
            });

        }


        $(function () {
            $('#dd').tooltip({
                content: $('<div></div>'),
                showEvent: 'click',
                onUpdate: function (content) {
                    content.panel({
                        width: 200,
                        border: false,
                        title: 'Login',
                        href: 'htmlPage.html'
                    });
                },
                onShow: function () {
                    var t = $(this);
                    t.tooltip('tip').unbind().bind('mouseenter', function () {
                        t.tooltip('show');
                    }).bind('mouseleave', function () {
                        t.tooltip('hide');
                    });
                }
            });
        });

        $(function () {
            $('#pg').pagination().find("a.l-btn").each(function () {

                if ($(this).find('span.icon-excel').length > 0) {
                    $(this).tooltip({
                        position: "top",
                        content: function () {
                            return "pagepagepagepagepage";
                        }
                    });
                }

            });

            $('#aaaaaaa').tooltip({
                //position: top,
                content: function () {
                    return "pagepagepagepagepage";
                }
            });

            $('#aaaaaaa').tooltip({
                position: top
            });

        });
    </script>

