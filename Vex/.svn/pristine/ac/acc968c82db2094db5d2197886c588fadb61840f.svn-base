    <script src="../../jquery-easyui-1.3.4/changeEasyuiTheme.js" type="text/javascript"></script>
    <link href="../../jquery-easyui-1.3.4/themes/icon.css" rel="stylesheet" type="text/css" />
    <script src="../../jquery-easyui-1.3.4/jquery.easyui.min.js" type="text/javascript"></script>
    <script src="../../jquery-easyui-1.3.4/js_hzycfg.js" type="text/javascript"></script>
    <script src="../../jquery-easyui-1.3.4/js_hzyCommon.js" type="text/javascript"></script>
    <script src="../../JS/MwqDemo/Button.js?Date=20131209" type="text/javascript"></script>

</head>
<body>
    <a href="#" id="A1" iconcls="icon-add" data-options="plain:true"
        class="easyui-linkbutton">添加</a>

    <a href="#" id="A3" iconcls="icon-edit" data-options="plain:true"
        class="easyui-linkbutton">编辑</a>

    <a href="#" id="A6" iconcls="icon-edit" data-options="plain:true"
        class="easyui-linkbutton">修改</a>


    <a href="#" id="A4" iconcls="icon-save" data-options="plain:true"
