            <form id="frmSeach">
                <input id="txtbrand" name="txtbrand" style="width: 200px" type="hidden">
                订货会季节：<input id="txtcmbdhhjj" name="txtcmbdhhjj" style="width: 200px" data-options="valueField:'id',textField:'text'">
                样衣号：<input id="txtdyyyno" name="txtdyyyno" style="width: 200px" data-options="valueField:'id',textField:'text'">
                <a href="#" id="btn_Search" iconcls="icon-search" onclick="SearchClick()" class="easyui-linkbutton"
                    data-options="plain:true">搜索</a>
            </form>
        </div>
    </div>

    <div region="center" data-options="split:true,border:true">
        <div class="easyui-layout" style="width: 100%; height: 100%;">
            <div region="west" data-options="split:true,border:true" style="width: 420px;">
                <table id="tabList">
                </table>
            </div>

            <div region="center">
                <form id="formSub" name="formSub" method="post">

                    <table style="font-size: 9pt;" cellpadding="2px;" id="tbSub">
                        <tr>
                            <td id="SampleClass">款别：<br />
                                <input id="EPVA" type="checkbox" checked="checked" />A
                                <input id="EPVB" type="checkbox" checked="checked" />B
                                <input id="EPVC" type="checkbox" checked="checked" />C
                                <input id="EPVD" type="checkbox" checked="checked" />D
