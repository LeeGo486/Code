//加载主页列表
function InitGrid(url, columns, orderName) {
    $('#tabList').datagrid({
        url: url,
        remoteSort: true,
        nowrap: true, //是否换行，True 就会把数据显示在一行里
        striped: true, //True 奇偶行使用不同背景色
        collapsible: false, //可折叠
        fit: true,
        showFooter: true,
        rownumbers: true,//行号
        singleSelect: true,//单行选择
        onDblClickRow: onDblClickRow,
        onClickCell: onClickCell,
        onSelect: onSelect,
        columns: columns,
        toolbar: [
            {
                id: 'btn_export',
                text: '导出',
                iconCls: 'icon-print',
                handler: function () {
                    excelExport();
                }
            },
            '-',
            {
                id: 'btn_save',
                text: '保存',
                iconCls: 'icon-save',
                handler: function () {
                    btnSave();
                }
            }
        ],
        onLoadSuccess: function (data) {
            if (data.rows.length == 1 && data.rows[0].category == '') {
                ClearGrid('#tabList');
                $.messager.alert('提示', '无相关数据', 'info');
            } else {
                var sumSample = 0;
                for (var i = 0; i < data.rows.length; i++) {
                    sumSample += IsNull(data.rows[i].samplenum);
                }
                var footer = [{
                    "category": "<font color='red' size='2'>合计</font>",

                    "samplenum": "<font color='red' size='2'>" + sumSample + "</font>"
                }];
                $('#tabList').datagrid('reloadFooter', footer);
            }
        },
        onLoadError: function () {
            $.messager.alert('提示', '加载失败', 'error');
        }
    });
}
