<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>登录页面</title>
    <meta charset="UTF-8">
    <script src="../../../jquery-easyui-1.3.4/jquery.min.js" type="text/javascript"></script>
    <script src="../../../jquery-easyui-1.3.4/jquery.cookie.js" type="text/javascript"></script>
    <link id="easyuiTheme" href="../../../jquery-easyui-1.3.4/themes/default/easyui.css"
        rel="stylesheet" type="text/css" /> 
    <link href="../../../jquery-easyui-1.3.4/themes/icon.css" rel="stylesheet" type="text/css" />
    <script src="../../../jquery-easyui-1.3.4/jquery.easyui.min.js" type="text/javascript"></script>
    <script src="../../../jquery-easyui-1.3.4/JS_PrintArea.js" type="text/javascript"></script>
    <script src="../../../jquery-easyui-1.3.4/js_hzyCommon.js" type="text/javascript"></script>
    <script src="../../../jquery-easyui-1.3.4/js_hzycfg.js" type="text/javascript"></script>
    <script type="text/javascript" src="../../../jquery-easyui-1.3.4/jquery.md5.js"></script>
    <script src="AttendanceIndex.js?Date=201319923" type="text/javascript"></script>
    <style>
        .input1 {
            width: 170px;
            font-size: 20pt;
            border: 1px solid #d4cdd9;
            color: #000; 
        }
         .input2 {
            width: 70px;
            font-size: 14pt;
            border: 1px solid #d4cdd9;
            color: #000; 
            margin-right:20px; 
        }
    </style>
</head>
<body style="background: url(../../../image/bg2.png) no-repeat; filter:alpha(opacity=90);
    -moz-opacity:0.9; -khtml-opacity: 0.9; opacity: 0.9; "> 
        <center> 
            <div style="width: 321px; height: 418px; margin-top:100px; margin-left:-65px;
                background: url(../../../image/dologin.png) no-repeat; " 
                onkeypress="if ( event.keyCode == 13 ){btndologin();return false;}" >  
                <div style="padding-top:160px;">
                    <form id="form_Login" >
                        <table  id="loginTable" style="font-size: 16pt;" cellpadding="10px" border="0">
                            <tr>
                                <td style="text-align:right;">
                                   账 号： 
                                </td>
                                <td style =" width:170px;">
                                   <input class="input1" type="text" id="UserName" name="UserName"  maxlength="20" />
                                </td>
                            </tr>
                            <tr>
                                <td style="text-align:right;">
                                   密 码： 
                                </td>
                                <td>
                                   <input class="input1" type="Password" id="Password" name="Password"  maxlength="20" />
                                </td>
                            </tr> 
                            <tr>
                                <td colspan="2">
                                    <input class="input2"  type="button" name="登录" value="登录" onclick="btndologin();" />
                                    <input class="input2"  type="button" name="重置" value="重置" onclick="doReset();" />
                                </td> 
                            </tr> 
                            <tr>
                                <td colspan="2">
                                    <div id="login_msg" style="text-align: center; 
                                        color: red; font-size: 12pt; padding: 2px; "></div>
                                </td> 
                            </tr> 
                        </table> 
                    </form>
                </div> 
        </div>
        </center> 
</body>
</html>
