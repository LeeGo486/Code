<script type="text/javascript">
//发展历程的JS代码
$(function(){
	$(".right").click(function(){
		turnright();
	});
	
	$(".left").click(function(){
		turnleft();
	}); 
});
var total = 5;
var cur = 0;
function turnleft(){
	pid = parseInt(cur)-1;
	if(pid<=total && pid>=0){
		getn(pid);
	}
/*	if(pid<total){
		$(".right").removeClass("gray");
	}
	if(pid==1){
		$(".left").addClass("gray");
	}
*/}

function turnright(){ 
	pid = parseInt(cur)+1;
	if(pid<=total && pid>0){
		getn(pid);
	}
}

function getn(pid){

	if(pid==total){
		$(".right").addClass("gray");
	}else{
		$(".right").removeClass("gray");
	}
	if(pid==0){
		$(".left").addClass("gray");
	}else{
		$(".left").removeClass("gray");
	}	

	var data = [
	{src:'/theme/orange/images/index/201401/zl-slider-1.jpg',target:'blank',href:'http://www.ports-intl.com/mens/product/shopbycategory/new/7703/new'},
	{src:'/theme/orange/images/index/201401/zl-slider-2.jpg',target:'blank',href:'http://www.ports-intl.com/mens/product/shopbycategory/new/8296/new'},
	{src:'/theme/orange/images/index/201401/zl-slider-3.jpg',target:'blank',href:'http://www.ports-intl.com/mens/product/shopbycategory/new/7863/new'}
	];
	cur = pid;
/*	$(".show").attr("rel",pid);
	if(pid==0){
		$(".show a").addClass('defaultcur');
	}else{
		$(".show a").removeClass('defaultcur');
	}
*/	//
//	$(".show img").attr("src",data[pid].src);
/*	if(cur == 0){
		$(".show a").bind('click',function(){turnright();return false;});
	}else{
		$(".show a").unbind('click');
	}*/
	$(".show img").animate({opacity:'toggle'},"10",null,function(){
		$(".show img").attr("src",data[pid].src);
		$(".show a").attr("href",data[pid].href);
		$(".show img").animate({opacity:'toggle'},"200");
	});

}


</script>
