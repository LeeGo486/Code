<!DOCTYPE html>
<html>
	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    
    	<link id="Link1" href="../../jquery-easyui-1.4.3/themes/bootstrap/easyui.css"
        	rel="stylesheet" type="text/css" />
    	<link href="../../jquery-easyui-1.4.3/themes/icon.css" rel="stylesheet" type="text/css" />
    
    <!-- <link href="../../css/SPWX_Examine.css" rel="stylesheet" type="text/css" /> -->
	    <script src="../../jquery-easyui-1.4.3/jquery.min.js" type="text/javascript"></script>
	    <script src="../../jquery-easyui-1.4.3/jquery.cookie.js" type="text/javascript"></script>
	    <script src="../../jquery-easyui-1.4.3/jquery.easyui.min.js" type="text/javascript"></script>
	    <script src="../../jquery-easyui-1.4.3/changeEasyuiTheme.js" type="text/javascript"></script>
	    
	    <script src="../../jquery-easyui-1.4.3/js_hzycfg.js" type="text/javascript"></script>
	    <script src="../../jquery-easyui-1.4.3/js_hzyCommon.js" type="text/javascript"></script>
	    <script src="../../JS/SPWX/SPWX_WXDetailInfo.js?Date=20150905" type="text/javascript"></script>
	    <style>
	        body{
	            background-color:#fff;
	            font-size:14px;
	            color:#5A5A5A;
	            font-family: "Helvetica","Tahoma","Arial",
	            "PingFang SC","Microsoft JhengHei","SimSun","sans-serif";
	        }
	        .area{
	        	margin-top:10px;
	        }
	        .name{
	        	font-size: 1.2em;
	        	margin-top: 10px;
	        	margin-left:10px;
	        }
	        .emphasize{
	        	color:#CC0033;
	        	font-weight:bold;
	        	font-size:1.2em;
	        }
	        table{
	        	margin-left:30px;
	        	/*border:solid 1px;*/
	        	/*width:90%;*/
	        }
	        th{
	        	text-align: left;
	        	font-weight:normal;
	        	padding-right: 10px;
	        }
	        td{
	        	text-align: left;
	        	line-height: 25px;
				padding-right: 20px;
	        }
    	</style>
	</head>
	<body>
		<div>
			<div class="area">
				<span class="name">维修单信息</span>
				<table>
					<tr>
					<th>维修单号:</th>
					<td id="wxno"></td>
					<th>紧急程度:</th>
					<td id="urgentlevel"></td>
					<th>维修仓:</th>
					<td id="warehouse"></td>
					<th>维修性质:</th>
					<td id="nature"></td>
					<th>维修单状态:</th>
					<td class="emphasize" id="status"></td>
					</tr>
				</table>
				<hr >
			</div>
			<div class="area">
				<span class="name">款式信息</span>
				<table>
					<tr>
						<th>款号:</th>
						<td id="sku"></td>
						<th>颜色:</th>
						<td id="color"></td>
						<th>尺码:</th>
						<td id="size"></td>
					</tr>
					<tr>
						<th>问题描述:</th>
						<td colspan="6" id="question"></td>
					</tr>
					<tr>
						<th>店铺备注:</th>
						<td colspan="6" id="remark"></td>
					</tr>
				</table>
				<hr >
			</div>
			<div class="area">
				<span class="name">判定信息</span>
				<table>
					<tr>
						<th>主要原因:</th>
						<td id="foname"></td>
						<th>主要问题:</th>
						<td id="fqname"></td>
						<th>主要现象:</th>
						<td id="fpname"></td>
						<th>维修区间:</th>
						<td id="dayarea"></td>
						<th>费用区间:</th>
						<td id="moneyarea"></td>
					</tr>
					<tr id="second">
						<th>次要原因:</th>
						<td id="soname"></td>
						<th>次要问题:</th>
						<td id="sqname"></td>
						<th>次要现象:</th>
						<td id="spname"></td>
					</tr>
					<tr>
						<th>责任部门:</th>
						<td id="dmname"></td>
						<th>判定结果:</th>
						<td id="drname"></td>
						<th>其他信息:</th>
						<td id="other"></td>
					</tr>
					<tr>
						<th>初判描述:</th>
						<td colspan="7" id="firstdecide"></td>
					</tr>
					<tr id="secondD">
						<th>再判描述:</th>
						<td colspan="7" id="sedDecide"></td>
					</tr>
					<tr id="thirdD">
						<th>最终描述:</th>
						<td colspan="7" id="trdDecide"></td>
					</tr>
				</table>
				<hr >
			</div>
			<div class="area">
				<span class="name">完成信息</span>
				<table>
					<tr>
						<td></td>
						<td></td>
					</tr>
				</table>
				<hr >
			</div>
			<div class="area">
				<span class="name">店铺信息</span>
				<table>
					<tr>
						<th>店铺大区:</th>
						<td id="area"></td>
						<th>店铺名称:</th>
						<td id="dpname"></td>
						<th>店铺电话:</th>
						<td id="dpphone"></td>
					</tr>
					<tr>
						<th>店长:</th>
						<td id="staff"></td>
						<th>导购:</th>
						<td id="resp"></td>
					</tr>
				</table>
				<hr >
			</div>
			<div class="area">
				<span class="name">客户信息</span>
				<table>
					<tr>
						<th>顾客姓名:</th>
						<td id="vipname"></td>
						<th>顾客电话:</th>
						<td id="vipphone"></td>
						<th>购买日期:</th>
						<td id="celldate"></td>
						<th>付费意愿:</th>
						<td id="cost"></td>
					</tr>
				</table>
				<hr >
			</div>
			<div class="area">
				<span class="name">物流信息</span>
				<table>
					<tr>
						<th>店铺:</th>
						<td colspan=7 id="dpExp"></td>
					</tr>
					<tr>
						<th>总部:</th>
						<td colspan=7 id="hwaExp"></td>
					</tr>
				</table>

			</div>
		</div>

		<!-- 维修完成 -->
		<div id="finishWin">
			<input type="text" name="txtisFinish" />
			<input type="text" name="txtfinishDate" />
			<input type="text" name="txtfinishMan" />
			<input type="text" name="txtfinishpoint" />
			<p></p>
		</div>
	</body>
</html>