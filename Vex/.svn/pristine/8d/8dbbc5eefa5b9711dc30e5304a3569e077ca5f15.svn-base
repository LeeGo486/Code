            <table style="margin-left:15px;margin-top:10px">
                <tr style="height: 40px">
                    <td style="text-align:right;"><span>公司编号：</span></td>
                    <td>
                        <input id="comid" type="text" style="width: 280px" disabled="disabled" /></td>
                </tr>
                <tr style="height: 40px">
                    <td style="text-align:right;"><span>公司名称：</span> </td>
                    <td>
                        <input id="comname" type="text" style="width: 280px"/></td>
                </tr>               
                <tr style="height: 40px">
                    <td style="text-align:right;"><span style="color: red;">人资姓名：</span></td>
                    <td>
                        <input id="rzempname" type="text" style="width: 180px" /></td>
                </tr>
                <tr style="height: 40px">
                    <td style="text-align:right;"><span style="color: red;">员工号：</span></td>
                    <td>
                        <input id="rzempcode" type="text" style="width: 180px" /></td>
                </tr>
                <tr style="height: 40px; text-align: center">
                    <td colspan="2">
                        <br>
                        <a href="javascript:void(0);" class="easyui-linkbutton" data-options="plain:true,iconCls:'icon-check-green'"
