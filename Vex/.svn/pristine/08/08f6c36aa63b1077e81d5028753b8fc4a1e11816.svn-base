<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>代销退厂管理</title>
    <script src="../../jquery-easyui-1.3.4/js_hzycfg.js" type="text/javascript"></script>
    <script src="../../jquery-easyui-1.3.4/jquery.min.js" type="text/javascript"></script>
    <script src="../../jquery-easyui-1.3.4/jquery.cookie.js" type="text/javascript"></script>
    <link id="easyuiTheme" href="../../jquery-easyui-1.3.4/themes/gray/easyui.css" rel="stylesheet" type="text/css" />
    <script src="../../jquery-easyui-1.3.4/changeEasyuiTheme.js" type="text/javascript"></script>
    <link href="../../jquery-easyui-1.3.4/themes/icon.css" rel="stylesheet" type="text/css" />
    <script src="../../jquery-easyui-1.3.4/jquery.easyui.min.js" type="text/javascript"></script>
    <script src="../../jquery-easyui-1.3.4/js_hzyCommon.js" type="text/javascript"></script>
    <script src="../../jquery-easyui-1.3.4/jquery.ajaxupload.js" type="text/javascript"></script>
    <script src="../../jquery-easyui-1.3.4/js.extend.datetime.js" type="text/javascript"></script>
    <script src="../../jquery-easyui-1.3.4/jquery.numberformatter.js" type="text/javascript"></script>
    <script src="../../JS/Hawk/Sellback.js?Date=20150508" type="text/javascript"></script>
    <script>
        $.fn.datebox.defaults.formatter = function (date) {
            var y = date.getFullYear();
            var m = date.getMonth() + 1;
            var d = date.getDate();
            return y + '-' + (m < 10 ? ('0' + m) : m) + '-' + (d < 10 ? ('0' + d) : d);
        }
        $.fn.datebox.defaults.parser = function (s) {
            if (!s) return new Date();
            var ss = (s.split('-'));
            var y = parseInt(ss[0], 10);
            var m = parseInt(ss[1], 10);
            var d = parseInt(ss[2], 10);
            if (!isNaN(y) && !isNaN(m) && !isNaN(d)) {
                return new Date(y, m - 1, d);
            } else {
                return new Date();
            }
        }
    </script>
</head>
<body class="easyui-layout" data-options="fit:true">
    <div data-options="region:'north'" style="height: 80px" title="筛选">
        <form id="formSelect">
            <table cellpadding="5">
                <tr>
                    <td>退厂单号</td>
                    <td>
                        <input id="txtSellbackCode" style="width: 100px" />
                    </td>
                    <td>供应商</td>
                    <td>
                        <input id="txtSupplier" style="width: 100px" />
                    </td>
                    <td>采购单号</td>
                    <td>
                        <input id="txtPOCode" style="width: 100px" />
                    </td>
                    <td>退货日期</td>
                    <td>
                        <input id="txtBeginDate" class="easyui-datebox" style="width: 100px" />
                        —
                        <input id="txtEndDate" class="easyui-datebox" style="width: 100px" />
                    </td>
                    <td>
                        <a class="easyui-linkbutton" data-options="iconCls:'icon-search',plain:true" onclick="btnSearchShopRetreatHead()">查询</a>
                    </td>
                    <td>
                        <a class="easyui-linkbutton" data-options="iconCls:'icon-cancel',plain:true">取消</a>
                    </td>
                </tr>
            </table>
        </form>
    </div>
    <div data-options="region:'center'" title="代销退厂列表">
        <table id="sellBackList"></table>
    </div>

    <div id="winEdit" title="代销退厂管理" class="easyui-window" data-options="closed:true,modal:true" style="width: 800px; height: 500px; top: 20px">
        <div class="easyui-layout" data-options="fit:true">
            <div data-options="region:'north'" style="height: 50px">
                <table cellpadding="5">
                    <tr>
                        <td>采购单号</td>
                        <td>
                            <input id="txtPOCode_edit" style="width: 100px" />
                        </td>
                        <td>到货计划单号</td>
                        <td>
                            <input id="txtAPOCode" style="width: 100px" />
                        </td>
                        <td>退货仓库</td>
                        <td>
                            <input id="txtWarehouse" style="width: 120px" />
                        </td>
                        <td>
                            <a class="easyui-linkbutton" data-options="iconCls:'icon-search',plain:true" onclick="btnSearch()">查询</a>
                        </td>
                        <td>
                            <a class="easyui-linkbutton" data-options="iconCls:'icon-cancel',plain:true" onclick="Cancel()">取消</a>
                        </td>
                    </tr>
                </table>
            </div>

            <div data-options="region:'center'">
                <table id="tabDataList"></table>
            </div>
        </div>
    </div>

    <div id="winDetail" title="代销退厂明细" class="easyui-window" data-options="closed:true,modal:true" style="width: 800px; height: 500px; top: 20px">
        <table id="tabDetail"></table>
    </div>

</body>
</html>
